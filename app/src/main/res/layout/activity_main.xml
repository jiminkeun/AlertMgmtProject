<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="daou.syscore.alertmgmtproject.MainActivity">

    <Button
        android:id="@+id/goCalculator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="계산기"/>

    <Button
        android:id="@+id/goCalculator2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:text="숫자계산기"
        app:layout_constraintStart_toEndOf="@+id/goCalculator"
        app:layout_constraintTop_toTopOf="@id/goCalculator"/>

    <Button
        android:id="@+id/goAlertMgmtList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:text="알람 관리 프로젝트"
        app:layout_constraintStart_toEndOf="@+id/goCalculator2"
        app:layout_constraintTop_toTopOf="@id/goCalculator2"/>

</android.support.constraint.ConstraintLayout>
